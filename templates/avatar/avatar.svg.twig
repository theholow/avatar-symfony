

<svg xmlns="http://www.w3.org/2000/svg"
     xmlns:xlink="http://www.w3.org/1999/xlink" viewBox="0 0 4 6">
    {% for rowIndex, row in grid %}
        {% for colIndex,color in row %}
            {% if color is defined %}
                <rect x="{{ colIndex }}" y="{{ rowIndex }}" height="1" width="1" fill="{{ color }}"/>
            {% endif %}
        {% endfor %}
    {% endfor %}
</svg>